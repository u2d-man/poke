{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@tanstack/eslint-plugin-query/recommended"
  ],
  "rules": {
    "react/no-unknown-property": ["error", {"ignore": ["css"]}],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "react/jsx-uses-react": "off",
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}
